<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Description>Working with non-generic collections</Description>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <Version>24.1.24.1</Version>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="SunamoAttributes" Version="24.1.24.1" />
    <PackageReference Include="SunamoCollectionsChangeContent" Version="24.1.14.2" />
    <PackageReference Include="SunamoCollectionsShared" Version="24.1.23.2" />
    <PackageReference Include="SunamoCollectionWithoutDuplicates" Version="24.1.14.1" />
    <PackageReference Include="SunamoDictionary" Version="24.1.14.1" />
    <PackageReference Include="SunamoInterfaces" Version="24.1.14.1" />
    <PackageReference Include="SunamoString" Version="24.1.22.1" />
    <PackageReference Include="SunamoStringParts" Version="24.1.23.6" />
  </ItemGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DefineConstants>$(DefineConstants);ASYNC</DefineConstants>
  </PropertyGroup>
</Project>